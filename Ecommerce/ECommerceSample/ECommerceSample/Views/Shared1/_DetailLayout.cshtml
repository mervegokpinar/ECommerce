@using ECommerce.Repository
@using ECommerce.Entity

<!DOCTYPE html>

<html dir="ltr" lang="tr">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>
    <!--#region Scripts & Contents  -->

    <base href=".">
    <script src="~/Content/web2/catalog/view/javascript/jquery-2.1.1.min.js"></script>
    <script src="~/Content/web2/catalog/view/javascript/bootstrap.min.js" type="text/javascript"></script>
    <link href="~/Content/web2/catalog/view/javascript/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href='//fonts.googleapis.com/css?family=Poppins:200,300,400,500,600,700,800' rel='stylesheet' type='text/css'>
    <link href="~/Content/web2/catalog/view/javascript/css/stylesheet.css" rel="stylesheet">

    <link rel="stylesheet" type="text/css" href="~/Content/web2/catalog/view/javascript/magnific-popup.css">
    <link rel="stylesheet" type="text/css" href="~/Content/web2/catalog/view/javascript/carousel.css">
    <link rel="stylesheet" type="text/css" href="~/Content/web2/catalog/view/javascript/custom.css">
    <link rel="stylesheet" type="text/css" href="~/Content/web2/catalog/view/javascript/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/web2/catalog/view/javascript/lightbox.css">
    <link rel="stylesheet" type="text/css" href="~/Content/web2/catalog/view/javascript/animate.css">

    <link href="style.href" type="text/css" rel="style.rel" media="style.media" />
    <link href="style.href" type="text/css" rel="style.rel" media="style.media" />

    <link href="~/Content/web2/catalog/view/javascript/swiper.min.css" type="text/css" rel="stylesheet" media="screen">
    <link href="~/Content/web2/catalog/view/javascript/opencart.css" type="text/css" rel="stylesheet" media="screen">
    <script src="~/Content/web2/catalog/view/javascript/swiper.jquery.js" type="text/javascript"></script>
    <link href="https://demo.codezeel.com/opencart/OPC05/OPC050122/image/catalog/cart.png" rel="icon">

    <script type="text/javascript" src="~/Content/web2/catalog/view/javascript/custom.js"></script>
    <script type="text/javascript" src="~/Content/web2/catalog/view/javascript/jstree.min.js"></script>
    <script type="text/javascript" src="~/Content/web2/catalog/view/javascript/carousel.min.js"></script>
    <script type="text/javascript" src="~/Content/web2/catalog/view/javascript/codezeel.min.js"></script>
    <script type="text/javascript" src="~/Content/web2/catalog/view/javascript/jquery.custom.min.js"></script>
    <script type="text/javascript" src="~/Content/web2/catalog/view/javascript/jquery.formalize.min.js"></script>
    <script type="text/javascript" src="~/Content/web2/catalog/view/javascript/lightbox-2.6.min.js"></script>
    <script type="text/javascript" src="~/Content/web2/catalog/view/javascript/tabs.js"></script>
    <script type="text/javascript" src="~/Content/web2/catalog/view/javascript/jquery.elevatezoom.min.js"></script>
    <script type="text/javascript" src="~/Content/web2/catalog/view/javascript/bootstrap-notify.min.js"></script>
    <script type="text/javascript" src="~/Content/web2/catalog/view/javascript/doubletaptogo.js"></script>
    <script type="text/javascript" src="~/Content/web2/catalog/view/javascript/owl.carousel.min.js"></script>
    <script type="text/javascript" src="~/Content/web2/catalog/view/javascript/jquery.magnific-popup.min.js"></script>

    <script src="~/Content/web2/catalog/view/javascript/common.js" type="text/javascript"></script>

    <!--#endregion -->
</head>

<body class="common-home layout-2 left-col">
    <nav id="top">

    </nav>
    <header>
     
    </header>
    <div class="wrap-breadcrumb parallax-breadcrumb">
        <div class="container page-container"></div>
    </div>

    <!-- ======= Quick view JS ========= -->
    <script>


        function quickbox() {
            if ($(window).width() > 767) {
                $('.quickview-button').magnificPopup({
                    type: 'iframe',
                    delegate: 'a',
                    preloader: true,
                    tLoading: 'Loading image #%curr%...',
                });
            }
        }
        jQuery(document).ready(function () { quickbox(); });
        jQuery(window).resize(function () { quickbox(); });

    </script>
    <section class="page-container container">

        <div id="common-home" class="">
            <div class="home-container">

            </div>

            <div class="row">
                <div id="content" class="col-sm-9">
                    <div class="box special">
                        <hr />
                        @RenderBody()
                    </div>
                </div>
            </div>
            <span class="special_default_width" style="display:none; visibility:hidden"></span>


            <span class="blog_default_width" style="display:none; visibility:hidden"></span>

        </div>


    </section>


</body>
</html>
